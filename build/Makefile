ifndef config
  config=release
endif
export config

PROJECTS := Libraries spatial_covid0d_estrat csv_to_input

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

Libraries: Libraries
	@echo "==== Building Libraries ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f Libraries.make

spatial_covid0d_estrat: Libraries
	@echo "==== Building spatial_covid0d_estrat ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f spatial_covid0d_estrat.make

csv_to_input: Libraries
	@echo "==== Building csv_to_input ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f csv_to_input.make

clean:
	@${MAKE} --no-print-directory -C . -f Libraries.make clean
	@${MAKE} --no-print-directory -C . -f spatial_covid0d_estrat.make clean
	@${MAKE} --no-print-directory -C . -f csv_to_input.make clean

help:
