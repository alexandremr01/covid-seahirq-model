	@echo "   all (default)"
	@echo "   clean"
	@echo "   Libraries"
	@echo "   spatial_covid0d_estrat"
	@echo "   csv_to_input"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
